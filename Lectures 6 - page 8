package Arrayyy;





public class baitapx2 {
 
    public static void main(String[] args) {
        int[] numbers;
        numbers = new int[8];
        
        for (int i = 0 ; i<8; i++) {
 
            
            
            
            numbers[i] = 2 * i;
            System.out.print(numbers[i] + " ");
            
        }
              
        
    }
}
